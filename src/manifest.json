{
  "manifest_version": 3,
  "name": "Ducket-CE",
  "description": "A chrome extension allowing bookmarks sharing from an hosted Markdown file.",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "permissions": [
    "storage",
    "alarms"
  ],
  "icons": {
    "128": "icon-128.png"
  }
}