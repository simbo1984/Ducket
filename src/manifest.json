{
  "manifest_version": 3,
  "name": "Ducket-CE",
  "description": "A chrome extension allowing bookmarks sharing from an hosted Markdown file.",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "ducket-box-logo-16.png",
      "24": "ducket-box-logo-24.png",
      "32": "ducket-box-logo-32.png",
      "48": "ducket-box-logo-48.png"
    },
    "default_title": "Click Me"
  },
  "permissions": [
    "storage",
    "alarms"
  ],
  "icons": {
    "16": "ducket-box-logo-16.png",
    "32": "ducket-box-logo-32.png",
    "48": "ducket-box-logo-48.png",
    "128": "ducket-box-logo-128.png"
  }
}